<div>
  <%= form_with url: searchs_search_path, method: :get do |f| %>
    大賞名検索：<%= f.text_field :content %>
    <%= button_tag type: "submit", class: "btn  btn-sm btn-dark" do %>
      <i class="fa-solid fa-magnifying-glass"></i>
    <% end %>
  <% end %>
</div>

<div class="ml-3">
  <%= form_with url: searchs_search_by_keyword_path, method: :get do |f| %>
    キーワード検索：<%= f.text_field :keyword %>
    <%= button_tag type: "submit", class: "btn  btn-sm btn-dark" do %>
      <i class="fa-solid fa-magnifying-glass"></i>
    <% end %>
  <% end %>
</div>